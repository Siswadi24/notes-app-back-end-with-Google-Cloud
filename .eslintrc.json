{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": ["error", "windows"],
        "max-len": ["error", { "code": 120 }]
        // "prefer-const": ["error", {
        //     "destructuring": "any",
        //     "ignoreReadBeforeAssign": false
        // }]
    }
}
